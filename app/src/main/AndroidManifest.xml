<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vegetablezooapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/applogo"
        android:label="Vegetable Zoo"
        android:roundIcon="@mipmap/applogo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LevelTwoFinish"></activity>
        <activity android:name=".GamePlayActivity2" />
        <activity android:name=".LevelOneFinish" />
        <activity android:name=".incorrectSplash" />
        <activity android:name=".correctSplash" />
        <activity android:name=".InstructActivity" />
        <activity android:name=".GamePlayActivity" />
        <activity android:name=".MainMenu" />
        <activity android:name=".TitleSplashScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>